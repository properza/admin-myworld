{
  "include": [
    "remix.env.d.ts",
    "**/*.ts",
    "src",
    "**/*.tsx",
    "app/routes/players._index.tsx"
  ],
  "compilerOptions": {
    "lib": ["dom", "es6"],
    "types": ["vitest/globals"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "commonjs", // Changed to lowercase for consistency
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "es2020", // Changed to lowercase for consistency
    "strict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    },
    "skipLibCheck": true,
    "noEmit": true
  }
}